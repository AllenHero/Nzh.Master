    <script type="text/javascript">
        $(function () {
            $("button[name=EditSelfPhoto]").click(function () {
                $("#modal_grxx").modal("hide");
                $("#avatar-modal").modal("show");
            });
            //手机号
            $("#userphone").change(function () {
                var phone = document.getElementById('userphone').value;
                if (!(/^1[3456789]\d{9}$/.test(phone))) {
                    $(this).next().html('<br><font class="tips_false">手机号格式不正确</font>');
                } else {
                    $(this).next().html('<br><font class="tips_true">输入正确</font>');
                }
            });
            $("button[name=editphone]").click(function () {
                if ($("#userphone").attr("disabled")) {
                    $("#userphone").removeAttr("disabled");
                    $(this).html("保存");
                } else {
                    var data = { UserId: UserId, TenantId: TenantId, PhoneNumber: '' };
                    if ((/^1[3456789]\d{9}$/.test($("#userphone").val()))) {
                        data.PhoneNumber = $("#userphone").val();
