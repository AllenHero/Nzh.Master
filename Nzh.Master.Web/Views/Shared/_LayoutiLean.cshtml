                        status = false;
                        toastr.warning("手机号格式不正确");
                        $("#userphone").next().html('<br><font class="tips_false">手机号格式不正确</font>');
                    }
                }
            });
            //第一次输入密码
            $("#divpassword1").change(function () {
                checkpsd1(this);
                if ($("#divpassword1").val() == $("#validation_password1").val()) {
                    $("#divpassword1").next().html('<br><font class="tips_false">新密码和原密码不能相同</font>');
                }
                if ($("#divpassword2").val()) {
                    if ($("#divpassword2").val() != $("#divpassword1").val())
                        $("#divpassword2").next().html('<br><font class="tips_false">两次密码不相同</font>');
                    else
                        $("#divpassword2").next().html('<br><font class="tips_true">输入正确</font>');
                }
            });
            //第二次输入密码
            $("#divpassword2").change(function () {
                checkpsd1(this);
                var a = $("#divpassword1").val();
                var b = $("#divpassword2").val();
                var c = $("#validation_password1").val();
                if (a != b) {
                    $("#divpassword2").next().html('<br><font class="tips_false">两次密码不相同</font>');
                } else if (a == c) {
                    $("#divpassword1").next().html('<br><font class="tips_false">新密码和原密码不能相同</font>');
                }
                else {
                    $("#divpassword2").next().html('<br><font class="tips_true">输入正确</font>');
                }
            });

            $("button[name=selfCancel]").click(function () {
                clearSelf();
            });

            $("button[name=selfSubmit]").click(function () {
                var data = checkValue();
                if (data) {
                    saveSelfInfoMation(data);
                }
            })
        });

        //验证密码 
        function checkpsd1(obj) {

            psd1 = $(obj).val();

            var flagZM = false;

            var flagSZ = false;

            var flagQT = false;

            if (psd1.length < 6 || psd1.length > 12) {

                $(obj).next().html('<br><font class="tips_false">长度错误</font>');

            } else {
                for (i = 0; i < psd1.length; i++) {
                    if ((psd1.charAt(i) >= 'A' && psd1.charAt(i) <= 'Z') || (psd1.charAt(i) >= 'a' && psd1.charAt(i) <= 'z')) {
                        flagZM = true;
                    }

                    else if (psd1.charAt(i) >= '0' && psd1.charAt(i) <= '9') {

                        flagSZ = true;

                    } else {
                        flagQT = true;

                    }
                }
                if (!flagZM || !flagSZ || flagQT) {

                    $(obj).next().html('<br><font class="tips_false">密码必须是字母数字的组合</font>');

                } else {
                    $(obj).next().html('<br><font class="tips_true">输入正确</font>');
                }
            }
        }

        //验证邮箱
        function checkmail() {
            apos = form1.youremail.value.indexOf("@@");
            dotpos = form1.youremail.value.lastIndexOf(".");
            if (apos < 1 || dotpos - apos < 2) {
                divmail.innerHTML = '<br><font class="tips_false">输入错误</font>';
            }
            else {
                divmail.innerHTML = '<br><font class="tips_true">输入正确</font>';
            }
        }

        // 判断是否为手机号
        function isPoneAvailable(pone) {
            var myreg = /^[1][3,4,5,7,8][0-9]{9}$/;
            if (!myreg.test(pone)) {
                return false;
            } else {
                return true;
            }
        };
        // 判断是否为电话号码
        function isTelAvailable(tel) {
            var myreg = /^(([0\+]\d{2,3}-)?(0\d{2,3})-)(\d{7,8})(-(\d{3,}))?$/;
            if (!myreg.test(tel)) {
                return false;
            } else {
                return true;
            }
        };

        //检查录入数据
        function checkValue() {
            var status = true;
            var data = { UserId: UserId, TenantId: TenantId, OldPwd: '', NewPwd: '' };

            if ($("#validation_password1").val()) {
                data.OldPwd = $("#validation_password1").val();
            } else {
                status = false;
                $("#validation_password1").next().html('<br><font class="tips_false">请输入旧密码</font>');
                toastr.warning("请输入旧密码");
            }
            if ($("#divpassword1").val() == $("#divpassword2").val() && $("#divpassword2").val()) {
                data.NewPwd = $("#divpassword2").val();
            } else {
                status = false;
                $("#divpassword2").next().html('<br><font class="tips_false">请重新输入新密码</font>');
                $("#divpassword1").next().html('<br><font class="tips_false">请重新输入新密码</font>');
                toastr.warning("请重新输入新密码");
            }

            if (status) {
                return data;
            } else {
                return '';
            }
        }
        //清除表单内容
        function clearSelf() {
            $("#userphone").next().html("");
            $("#divpassword2").next().html('<br><font>两次密码需要相同</font>');
            $("#divpassword1").next().html('<br><font>密码必须由字母和数字组成</font>');
            $("input[group=self]").val("");
        }

        function saveSelfInfoMation(data) {
            $.ajax({
                url: '/api/sys/user/EditSelfInfomation',
                type: 'Post',
                dataType: 'json',
                data: JSON.stringify(data),
                contentType: 'application/json',
                success: function (value) {
                    if (value.status) {
                        var phone = $("#userphone").val();
                        $("#modal_grxx").modal("hide");
                        //清除表单内容
                        clearSelf();
                        $("#userphone").attr("disabled");
                        $("#userphone").val(phone);
                        $("button[name=editphone]").html("编辑");
                    } else {
                        toastr.warning(value.message);
                    }
                }
            });
        }
    </script>
