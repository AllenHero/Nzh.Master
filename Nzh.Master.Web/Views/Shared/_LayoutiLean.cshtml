    <style>
        .required {
            border-color: #ef8c8c !important;
            background-color: #f2d1d1 !important;
            border-radius: 3px;
        }

        .oDiv {
            height: 200px;
            width: 205px;
            border: 1px solid #dcdfe6;
            border-radius: 4px;
            text-align: center;
            margin-bottom: 20px;
        }

        .oInput {
            border: solid 1px #dcdfe6;
            line-height: 34px;
        }
        /*a  upload */
        .a-upload {
            padding: 0px 15px;
            height: 34px;
            line-height: 34px;
            position: relative;
            cursor: pointer;
            background: #ffffff !important;
            border: 1px solid #dcdfe6 !important;
            box-shadow: none !important;
            overflow: hidden;
            display: inline-block;
            border-radius: 4px;
            *display: inline;
            *zoom: 1
        }

            .a-upload input {
                position: absolute;
                font-size: 100px;
                right: 0;
                top: 0;
                opacity: 0;
                filter: alpha(opacity=0);
                cursor: pointer
            }

            .a-upload:hover {
                text-decoration: none;
                background: #ddecfc !important;
                border: 1px solid #409eff !important;
                color: #208af7;
                -webkit-transition: background-color 0.7s !important;
                transition: background-color 0.7s !important;
                transition-property: background-color !important;
                transition-duration: 0.7s !important;
                transition-timing-function: ease !important;
                transition-delay: 0s !important;
            }
        /*密码验证*/
        .contact ul li input:focus {
            border-color: #c00;
        }

        .tips_true {
            color: green;
        }

        .tips_false {
            color: red;
        }

        /*logo*/
        #auto_image img {
            max-width: 40px;
            autoimg: expression(onload=function(){this.style.width=(this.offsetWidth > 40)?"40px":"auto"});
        }

        #nav_auto_image {
            display: table-cell;
            vertical-align: middle;
            line-height: 40px;
            padding: 0px 20px;
        }

            #nav_auto_image img {
                max-width: 30px;
                autoimg: expression(onload=function(){this.style.width=(this.offsetWidth > 30)?"30px":"auto"});
            }

        .required {
            border-color: #ef8c8c !important;
            background-color: #f2d1d1 !important;
            border-radius: 3px;
        }
    </style>
